{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "Node",
    "baseUrl": "./",
    "paths": {
      "types": ["@nuxt/types"],
      "@/*": ["./*"],
      "#app": ["./node_modules/nuxt3/dist/app/index"]
    },
    "noUncheckedIndexedAccess": true
  },
}
